{
  "name": "@cpannwitz/nestjs-example-starter",
  "version": "0.0.1",
  "description": "NodeJS server powered by NestJS",
  "author": {
    "email": "ch.von.pa@gmail.com",
    "name": "Christian Pannwitz",
    "url": "https://pnwtz.me"
  },
  "license": "MIT",
  "engines": {
    "node": "12.14.1",
    "yarn": "1.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css}": [
      "yarn format",
      "yarn lint"
    ]
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:docker": "docker-compose up -V -d && docker-compose logs -f node",
    "start:docker:full": "docker-compose up --build -V -d && docker-compose logs -f node",
    "stop:docker": "docker-compose down",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/main",
    "start:prod:migrations": "run-s migrations:run:prod start:prod",
    "lint": "./node_modules/.bin/eslint \"src/**/*\" --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css}\"",
    "version": "auto-changelog -p -u -t keepachangelog && git add .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:reset": "run-s db:clean migrations:run db:seed",
    "db:clean": "rimraf .data",
    "db:seed": "ts-node src/_db/run-seed",
    "schema:generate:dev": "ts-node src/generate-typings",
    "schema:generate:prod": "node dist/generate-typings",
    "migrations:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n $NAME",
    "migrations:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n $NAME",
    "migrations:show": "ts-node ./node_modules/typeorm/cli.js migration:show",
    "migrations:run:dev": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migrations:run:prod": "typeorm migration:run"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.3.1",
    "@nestjs/common": "^6.11.11",
    "@nestjs/config": "^0.2.4",
    "@nestjs/core": "^6.11.11",
    "@nestjs/graphql": "^6.6.2",
    "@nestjs/jwt": "^6.1.2",
    "@nestjs/passport": "^6.2.0",
    "@nestjs/platform-express": "^6.11.11",
    "@nestjs/swagger": "^4.3.2",
    "@nestjs/terminus": "^6.5.6",
    "@nestjs/typeorm": "^6.3.3",
    "@nestjsplus/redirect": "^1.0.0",
    "apollo-server-express": "^2.11.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.1",
    "graphql": "^14.6.0",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.0.14",
    "graphql-subscriptions": "^1.1.0",
    "graphql-tools": "^4.0.7",
    "helmet": "^3.21.3",
    "nest-access-control": "^2.0.1",
    "nestjs-pino": "^1.1.3",
    "nestjs-redis": "^1.2.5",
    "node-mocks-http": "^1.8.1",
    "npm-run-all": "^4.1.5",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-google-token": "^0.1.2",
    "passport-jwt": "^4.0.0",
    "pg": "^7.18.2",
    "pino-pretty": "^3.6.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.3",
    "type-graphql": "^0.17.6",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.14.2",
    "@nestjs/schematics": "^6.9.4",
    "@nestjs/testing": "^6.11.11",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.3",
    "@types/express-rate-limit": "^5.0.0",
    "@types/graphql": "^14.5.0",
    "@types/helmet": "^0.0.45",
    "@types/jest": "^25.1.3",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/node": "^13.7.7",
    "@types/passport": "^1.0.2",
    "@types/passport-google-oauth20": "^2.0.3",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "eslint": "^6.8.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "keepachangelog": "^0.7.2",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
